{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/(.*)\\.html",
      "destination": "/$1",
      "statusCode": 301
    },
    {
      "source": "/(.*)\\.php",
      "destination": "/$1",
      "statusCode": 301
    }
  ],
  "error": true
}
